{
  "name": "credit-card",
  "version": "0.0.1",
  "description": "Stencil Credit Card Web Component",
  "main": "dist/index.cjs.js",
  "module": "dist/credit-card/index.js",
  "es2015": "dist/esm/index.mjs",
  "es2017": "dist/esm/index.mjs",
  "types": "dist/custom-elements/index.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "collection:main": "dist/collection/index.js",
  "unpkg": "dist/credit-card/credit-card.js",
  "files": [
    "dist/",
    "loader/"
  ],
  "scripts": {
    "build": "stencil build --docs",
    "start": "stencil build --dev --watch --serve",
    "test": "stencil test --spec --e2e",
    "test.watch": "stencil test --spec --e2e --watchAll",
    "generate": "stencil generate",
    "s3-delete": "aws s3 rm s3://microfrontends-cdn/webcomponents/credit-card/v1 --recursive",
    "s3-upload": "aws s3 cp dist/v1/credit-card s3://microfrontends-cdn/webcomponents/credit-card/v1 --recursive",
    "publish": "npm run build && npm run s3-delete && npm run s3-upload"
  },
  "dependencies": {
    "@stencil/core": "^2.0.1"
  },
  "license": "MIT",
  "devDependencies": {}
}
