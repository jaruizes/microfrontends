{
  "name": "mf-accounts-summary",
  "version": "1.0.0",
  "author": "Jos√© Alberto Ruiz <jalb80@gmail.com> (https://jaruiz.io)",
  "scripts": {
    "ng": "ng",
    "mocks-server": "json-server --routes mocks/routes.json --port 3005 --watch mocks/api-mock.json &",
    "start": "ng serve --port 4205",
    "build:prod": "rimraf dist && ng build --prod --output-hashing=none --single-bundle",
    "build": "ng build --output-hashing=none --single-bundle",
    "test": "npm run build && cd dist/mf-accounts-summary && npx static-server",
    "expose": "npm run mocks-server && cd dist/mf-accounts-summary/v1 && npx static-server",
    "lint": "ng lint",
    "e2e": "start-server-and-test start http://localhost:4205 cypress:run",
    "cypress:run": "cypress run",
    "cypress:open": "cypress open",
    "s3-delete-js": "aws s3 rm s3://technology-bank-microfrontends-bucket/microfrontends/mf-accounts-summary/v1 --recursive",
    "s3-delete-assets": "aws s3 rm s3://technology-bank-microfrontends-bucket/assets/microfrontends/mf-accounts-summary/v1 --recursive",
    "s3-upload-js": "aws s3 cp dist/mf-accounts-summary/v1 s3://technology-bank-microfrontends-bucket/microfrontends/mf-accounts-summary/v1 --recursive --exclude 'assets/*'",
    "s3-upload-assets": "aws s3 cp dist/mf-accounts-summary/v1/assets/microfrontends s3://technology-bank-microfrontends-bucket/assets/microfrontends --recursive",
    "s3-delete": "npm run s3-delete-assets && npm run s3-delete-js",
    "s3-upload": "npm run s3-upload-assets && npm run s3-upload-js",
    "publish": "npm run s3-delete && npm run s3-upload",
    "publish:local": "npm run build:prod && cp -R ./dist/ ../../platform/mf-server/microfrontends-server/content/microfrontends/"
  },
  "private": true,
  "dependencies": {
    "@angular-extensions/elements": "^10.0.2",
    "@angular/animations": "~10.1.0",
    "@angular/cdk": "^10.2.0",
    "@angular/common": "~10.1.0",
    "@angular/compiler": "~10.1.0",
    "@angular/core": "~10.1.0",
    "@angular/elements": "^10.1.0",
    "@angular/flex-layout": "^10.0.0-beta.32",
    "@angular/forms": "~10.1.0",
    "@angular/platform-browser": "~10.1.0",
    "@angular/platform-browser-dynamic": "~10.1.0",
    "@ng-bootstrap/ng-bootstrap": "~7.0.0",
    "@ngx-translate/core": "^13.0.0",
    "@ngx-translate/http-loader": "^6.0.0",
    "@auth0/angular-jwt": "^5.0.1",
    "@popperjs/core": "^2.4.4",
    "bootstrap": "^4.5.2",
    "jquery": "^3.5.1",
    "rxjs": "~6.6.0",
    "tslib": "^2.0.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1001.0",
    "@angular/cli": "~10.1.0",
    "@angular/compiler-cli": "~10.1.0",
    "@types/jasmine": "~3.5.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.11.1",
    "codelyzer": "^6.0.0",
    "cypress": "^5.5.0",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "json-server": "^0.16.1",
    "karma": "~5.0.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "ngx-build-plus": "^10.1.1",
    "protractor": "~7.0.0",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typescript": "~4.0.2",
    "start-server-and-test": "^1.11.5"
  }
}
