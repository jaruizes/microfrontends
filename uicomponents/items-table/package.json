{
  "name": "items-table",
  "version": "1.0.0",
"author": "Jos√© Alberto Ruiz <jalb80@gmail.com> (https://jaruiz.io)",
  "description": "Stencil items-table",
  "main": "dist/v1/index.cjs.js",
  "es2015": "dist/esm/index.mjs",
  "es2017": "dist/esm/index.mjs",
  "scripts": {
    "build": "stencil build --docs",
    "build:prod": "stencil build --docs",
    "start": "stencil build --dev --watch --serve",
    "test": "stencil test --spec --e2e",
    "test.watch": "stencil test --spec --e2e --watchAll",
    "generate": "stencil generate",
    "s3-delete": "aws s3  rm s3://jalb80-microfrontends-bucket/uicomponents/items-table/v1 --recursive",
    "s3-upload": "aws s3  cp dist/v1/items-table s3://jalb80-microfrontends-bucket/uicomponents/items-table/v1 --recursive",
    "publish": "npm run s3-delete && npm run s3-upload"
  },
  "dependencies": {
    "@popperjs/core": "^2.4.4",
    "@stencil/core": "^2.0.1",
    "bootstrap": "^4.5.2",
    "jquery": "^3.5.1"
  },
  "license": "MIT",
  "devDependencies": {
    "@stencil/sass": "^1.3.2"
  }
}
