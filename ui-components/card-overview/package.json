{
  "name": "card-overview",
  "version": "0.0.1",
  "description": "Stencil card overview",
  "main": "dist/v1/index.cjs.js",
  "es2015": "dist/esm/index.mjs",
  "es2017": "dist/esm/index.mjs",
  "types": "dist/custom-elements/index.d.ts",
  "scripts": {
    "build": "stencil build --docs",
    "start": "stencil build --dev --watch --serve",
    "test": "stencil test --spec --e2e",
    "test.watch": "stencil test --spec --e2e --watchAll",
    "generate": "stencil generate",
    "s3-delete": "aws s3 rm s3://microfrontends-cdn/webcomponents/card-overview/v1 --recursive",
    "s3-upload": "aws s3 cp dist/v1/card-overview s3://microfrontends-cdn/webcomponents/card-overview/v1 --recursive",
    "publish": "npm run build && npm run s3-delete && npm run s3-upload"
  },
  "dependencies": {
    "@popperjs/core": "^2.4.4",
    "@stencil/core": "^2.0.1",
    "bootstrap": "^4.5.2",
    "jquery": "^3.5.1"
  },
  "license": "MIT",
  "devDependencies": {
    "@stencil/sass": "^1.3.2"
  }
}
