{
  "name": "backofiiceapp",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "mocks-server": "json-server --routes mocks/routes.json --watch mocks/api-mock.json --port 3050 &",
    "start": "ng serve --port 4250",
    "build": "ng build",
    "build:prod": "rimraf dist && ng build --prod",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "s3-delete": "aws s3 rm s3://microfrontends-applications-backoffice --recursive",
    "s3-upload": "aws s3 cp dist/backoffice s3://microfrontends-applications-backoffice --recursive",
    "publish": "npm run build:prod && npm run s3-delete && npm run s3-upload"
  },
  "private": true,
  "dependencies": {
    "@angular-extensions/elements": "^10.0.2",
    "@angular/animations": "~10.1.0",
    "@angular/cdk": "^10.2.0",
    "@angular/common": "~10.1.0",
    "@angular/compiler": "~10.1.0",
    "@angular/core": "~10.1.0",
    "@angular/flex-layout": "^10.0.0-beta.32",
    "@angular/forms": "~10.1.0",
    "@angular/material": "^10.2.0",
    "@angular/platform-browser": "~10.1.0",
    "@angular/platform-browser-dynamic": "~10.1.0",
    "@angular/router": "~10.1.0",
    "@fortawesome/fontawesome-free": "5.13.1",
    "@ng-bootstrap/ng-bootstrap": "~7.0.0",
    "@ngx-translate/core": "^13.0.0",
    "@ngx-translate/http-loader": "^6.0.0",
    "@popperjs/core": "^2.4.4",
    "angular-oauth2-oidc": "^10.0.3",
    "bootstrap": "^4.5.2",
    "flag-icon-css": "^3.5.0",
    "jquery": "^3.5.1",
    "json-server": "^0.16.1",
    "keycloak-js": "^11.0.2",
    "ngx-flag-icon-css": "^1.0.1",
    "rxjs": "~6.6.0",
    "simple-line-icons": "^2.4.1",
    "tslib": "^2.0.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1001.0",
    "@angular/cli": "~10.1.0",
    "@angular/compiler-cli": "~10.1.0",
    "@types/node": "^12.11.1",
    "@types/jasmine": "~3.5.0",
    "@types/jasminewd2": "~2.0.3",
    "codelyzer": "^6.0.0",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~5.0.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "protractor": "~7.0.0",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typescript": "~4.0.2"
  }
