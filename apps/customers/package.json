{
  "name": "customersapp",
  "version": "1.0.0",
  "author": "Jos√© Alberto Ruiz <jalb80@gmail.com> (https://jaruiz.io)",
  "scripts": {
    "ng": "ng",
    "mocks-server": "json-server --port 3200 --routes ../../platform/local/mocks-server/mocks/routes.json --watch ../../platform/local/mocks-server/mocks/api-mock.json &",
    "start": "npm run mocks-server && ng serve",
    "start:local": "npm run mocks-server && ng serve --configuration=local --port 4250 --proxy-config=proxy.conf-local.json",
    "build": "ng build",
    "build:prod": "rimraf dist && ng build --prod",
    "build:prod-local": "rimraf dist && ng build --prod --configuration=local",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "s3-delete": "aws s3 rm s3://technology-bank-microfrontends-applications-customers --recursive",
    "s3-upload": "aws s3 cp dist/customers s3://technology-bank-microfrontends-applications-customers --recursive",
    "publish": "npm run s3-delete && npm run s3-upload"
  },
  "private": true,
  "dependencies": {
